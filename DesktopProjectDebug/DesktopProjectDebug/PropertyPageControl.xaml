<UserControl x:Class="DesktopProjectDebug.PropertyPageControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DesktopProjectDebug"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel x:Name="rootPanel" Margin="10,10,0,0"
        Orientation="Horizontal" VerticalAlignment="Top">
        <TextBox x:Name="selectedAppServiceTextBox" Padding="5">Selected Azure App Service</TextBox>
        <Button Margin="10,0,0,0" Padding="5">Click to Select</Button>
    </StackPanel>
</UserControl>
