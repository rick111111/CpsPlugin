<UserControl x:Class="Microsoft.VisualStudio.Debugger.Parallel.Extension.PropertyPageControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Microsoft.VisualStudio.Debugger.Parallel.Extension"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
        <Label Content="Target App Service:" />
        <TextBlock Padding="5">
            <Hyperlink Click="Hyperlink_Click">
                <Run Text="{Binding AppServiceName}" />
            </Hyperlink>            
        </TextBlock>
    </StackPanel>
</UserControl>
